<h1>Kinect Test</h1>
<div class="kinect-test">
    {{#kinect-listener
       class="kinect-listener"
       as |bodies|}}
        {{kinect-visualizer
          class="kinect-visualizer"
          width=600
          height=600
          bodies=bodies}}
        {{kinect-debugger
          class="kinect-debugger"
          bodies=bodies}}
    {{/kinect-listener}}
</div>

{{outlet}}
